{

  "manifest_version": 2,
  "name": "ncrandom",
  "version": "1.0",

  "description": "Filter random questions from Neetcode list",
  
  "content_scripts": [
    {
      "matches": ["*://*.neetcode.io/*"],
      "js": ["dialog.js", "ncrandom.js"]
    }
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Neetcode Random",
    "default_popup": "popup.html"
  }
  
}
